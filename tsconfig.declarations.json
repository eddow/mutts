{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"declaration": true,
		"declarationMap": false,
		"emitDeclarationOnly": true,
		"noEmitOnError": false,
		"skipLibCheck": true,
		"strict": false,
		"noImplicitAny": false,
		"noImplicitReturns": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
	}
}
